            "name": "Debug (Launch)-ConfigServiceApplication<config-service>",
            "request": "launch",
            "mainClass": "com.example.configservice.ConfigServiceApplication",
            "projectName": "config-service"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-ReservationServiceApplication<reservation-service>",
            "request": "launch",
            "mainClass": "com.example.reservationservice.ReservationServiceApplication",
            "projectName": "reservation-service"
        },
        {
            "type": "java",
            "name": "Spring Boot-EurekaServiceApplication<eureka-service>",
