            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.example.eurekaservice.EurekaServiceApplication",
            "projectName": "eureka-service",
            "args": ""
        },
        {
            "type": "java",
            "name": "Spring Boot-ReservationClientApplication<reservation-client>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
